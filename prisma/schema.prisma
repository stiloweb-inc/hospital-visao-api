generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Location {
  id String @id @default(nanoid())

  name    String?
  address String
  city    String?
  state   String?
  cep     String?
  lat     String?
  long    String?

  createdAt DateTime @default(now())

  @@map("locations")
}

model Specialty {
  id String @id @default(nanoid())

  name String

  createdAt DateTime @default(now())

  @@map("specialities")
}
