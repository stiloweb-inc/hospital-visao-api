{
  "name": "hospital-visao-api",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bunx prisma generate",
    "deploy": "bunx prisma migrate deploy && bun run start",
    "db:seed": "bun run prisma/seed.ts",
    "db:reset": "bunx prisma migrate reset --force && bun run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "elysia": "latest",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "bun-types": "latest"
  },
  "module": "src/index.js",
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  }
}
